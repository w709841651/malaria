    ")\n",
    "\n",
    "print(len(data))\n",
    "data.head()\n",
    "\n",
    "target = data[['PRE']]\n",
    "features = data.drop('PRE', axis=1) \n",
    "features = features.drop('Tail.Length', axis=1)\n",
    "features = features.drop(['Wing.Length', 'pop.decreasing', 'pop.stable', 'pop.increasing','mig_strictmig','mig_partialmigrant','mig_resident'], axis=1)\n",
